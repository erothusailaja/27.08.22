void subString(char str[], int n)
{
	int x,i,k,j;
	for(i=1;i<=n;i++)
	{
		for(j=0;j<=n-i;j++)
		{
			int x = j+i;
			for(k=j;k<x;k++)
			   printf("%3c",str[k]);
			printf("\n");
		}
	}
}

// driver program to test above function
int main()
{
	char str[] = "abcd";
	subString(str, strlen(str));
	return 0;
}
