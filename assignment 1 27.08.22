#include<stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
	char s1[20];
	printf("Enter a string");
	gets(s1);
	int i,j;
	int n;
	for(i=0;i<n-1;i++)
	{
		for(j=j+1;j<n;j++)
		{
			char temp;
			if(s1[i]>s1[j])
			{
			temp=s1[i];
		    s1[i]=s1[j];
		    s1[j]=temp;	
			}
		}
	}
	puts(s1);
	return 0;
}
